{
  "version": 2,
  "routes": [
    { "src": "/api/proxy/(.*)", "dest": "/api/proxy/$1" },
    { "src": "/api/(.*)", "dest": "https://infamous-freight-ai.fly.dev/api/$1" }
  ],
  "env": {
    "NODE_VERSION": "20.18.1",
    "NEXT_PUBLIC_ENV": "production",
    "NEXT_PUBLIC_API_BASE": "https://infamous-freight-ai.fly.dev"
  }
}
