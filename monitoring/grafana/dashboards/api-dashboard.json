{
  "dashboard": {
    "title": "Infamous Freight - API Performance",
    "tags": ["api", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{path}}"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 2,
        "title": "Response Time (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m])",
            "legendFormat": "5xx errors"
          },
          {
            "expr": "rate(http_requests_total{status=~\"4..\"}[5m])",
            "legendFormat": "4xx errors"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 8,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 4,
        "title": "Active Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "socket_io_connections",
            "legendFormat": "WebSocket connections"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 8,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 5,
        "title": "Database Query Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(prisma_query_duration_seconds_sum[5m]) / rate(prisma_query_duration_seconds_count[5m])",
            "legendFormat": "Average query time"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 16,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes",
            "legendFormat": "Memory usage"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 16,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 7,
        "title": "AI Service Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ai_dispatch_recommendations_total[5m])",
            "legendFormat": "Dispatch recommendations"
          },
          {
            "expr": "rate(ai_coaching_sessions_total[5m])",
            "legendFormat": "Coaching sessions"
          }
        ],
        "gridPos": {
          "x": 0,
          "y": 24,
          "w": 12,
          "h": 8
        }
      },
      {
        "id": 8,
        "title": "Shipment Processing",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(shipments_processed_total[1h]))",
            "legendFormat": "Shipments/hour"
          }
        ],
        "gridPos": {
          "x": 12,
          "y": 24,
          "w": 6,
          "h": 4
        }
      },
      {
        "id": 9,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(redis_cache_hits_total[5m]) / (rate(redis_cache_hits_total[5m]) + rate(redis_cache_misses_total[5m]))",
            "legendFormat": "Hit rate"
          }
        ],
        "gridPos": {
          "x": 18,
          "y": 24,
          "w": 6,
          "h": 4
        }
      }
    ],
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
