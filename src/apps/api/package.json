{
  "name": "infamous-freight-api",
  "version": "1.0.0",
  "description": "Backend API for Infamous Freight Enterprises",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "pnpm run prisma:generate",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "echo 'Linting handled by root eslint config'",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "node ./scripts/prisma-generate.mjs",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "^5.15.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^3.0.3",
    "pdfkit": "^0.15.0",
    "rate-limiter-flexible": "^5.0.3",
    "redis": "^4.7.0",
    "socket.io": "^4.8.1",
    "stripe": "^20.1.0",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.9.3",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.13.5",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prisma": "^5.15.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  }
}
