{
  "name": "infamous-freight-api",
  "version": "2.0.0",
  "description": "REST API for freight management platform with authentication, billing, AI integration, and voice features",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "node src/server.js",
    "start": "NODE_ENV=production node src/server.js",
    "lint": "echo 'Linting handled by root eslint config'",
    "lint:fix": "echo 'Linting handled by root eslint config'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "validate:env": "node scripts/env.validation.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@infamous-freight/shared": "file:../packages/shared",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "^5.11.0",
    "@sentry/node": "^7.100.0",
    "axios": "^1.7.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^5.2.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "openai": "^4.0.0",
    "rate-limiter-flexible": "^2.4.2",
    "stripe": "^12.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "express-validator": "^7.3.1",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "prisma": "^5.11.0",
    "supertest": "^7.1.4"
